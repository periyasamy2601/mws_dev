{
  "name": "avk-api",
  "version": "0.0.1",
  "description": "Project",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t avk-api .",
    "docker:run": "docker run -p 3000:3000 -d avk-api",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "minify": "uglifyjs-folder dist/ -e -o release/ -x .js",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "dev-migrate": "export NODE_ENV=development || set NODE_ENV=development&& node dist/migrate",
    "stage-migrate": "export NODE_ENV=stage && node dist/migrate",
    "prod-migrate": "export NODE_ENV=production && node dist/migrate",
    "dev": "export NODE_ENV=development || set NODE_ENV=development&& npm run clean && npm run build && npm run start:watch",
    "stage": "export NODE_ENV=stage || set NODE_ENV=stage&& npm run rebuild && node build/dist/index.js",
    "prod": "export NODE_ENV=production || set NODE_ENV=production&& npm run rebuild && node build/dist/index.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "senthilkumar <senthilkumar.j@mobitechwireless.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^7.0.9",
    "@loopback/core": "^6.1.6",
    "@loopback/repository": "^7.0.9",
    "@loopback/rest": "^14.0.9",
    "@loopback/rest-explorer": "^7.0.9",
    "@loopback/service-proxy": "^7.0.9",
    "dotenv": "^17.2.2",
    "loopback-connector-postgresql": "^7.2.3",
    "tsc-watch": "^7.1.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.8",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/testlab": "^7.0.8",
    "@types/node": "^16.18.119",
    "eslint": "^8.57.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
