{
  "name": "@periyasamy2601/mws_dev",
  "version": "1.0.0",
  "description": "mws_dev",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__/acceptance/program-status.test.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t mws-dconag-api .",
    "docker:run": "docker run -p 3000:3000 -d mws-dconag-api",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "minify": "uglifyjs-folder dist/ -e -o release/ -x .js",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "dev-migrate": "export NODE_ENV=development || set NODE_ENV=development&& node dist/migrate",
    "stage-migrate": "export NODE_ENV=stage && node dist/migrate",
    "prod-migrate": "export NODE_ENV=production && node dist/migrate",
    "dev": "export NODE_ENV=development || set NODE_ENV=development&& npm run clean && npm run build && npm run start:watch",
    "stage": "export NODE_ENV=stage || set NODE_ENV=stage&& npm run rebuild && node build/dist/index.js",
    "prod": "export NODE_ENV=production || set NODE_ENV=production&& npm run rebuild && node build/dist/index.js",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@aws-sdk/lib-storage": "^3.665.0",
    "@loopback/authentication": "^11.0.6",
    "@loopback/authentication-jwt": "^0.15.6",
    "@loopback/boot": "^7.0.6",
    "@loopback/cli": "^6.2.4",
    "@loopback/core": "^6.1.3",
    "@loopback/cron": "^0.12.6",
    "@loopback/repository": "^7.0.4",
    "@loopback/rest": "^14.0.6",
    "@loopback/rest-explorer": "^7.0.6",
    "@loopback/service-proxy": "^7.0.4",
    "@types/nodemailer": "^6.4.17",
    "amqplib": "0.10.3",
    "axios": "^1.7.2",
    "define-properties": "^1.2.1",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "firebase-admin": "^12.6.0",
    "fs": "^0.0.1-security",
    "loopback-connector-postgresql": "^7.1.5",
    "loopback-connector-rest": "^5.0.1",
    "loopback4-soft-delete": "^10.0.2",
    "luxon": "^3.4.4",
    "mocha": "^10.7.3",
    "moment": "^2.29.4",
    "mqtt": "^5.10.1",
    "multer": "^1.4.5-lts.1",
    "@periyasamy2601/mws_dev": "1.0.0",
    "nodemailer": "^6.9.16",
    "pdfmake": "^0.2.13",
    "phone": "^3.1.50",
    "redis": "^5.6.0",
    "sharp": "^0.33.5",
    "tslib": "^2.7.0",
    "twilio": "^5.3.3",
    "useragent": "^2.3.0",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.6",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/testlab": "^7.0.6",
    "@types/amqplib": "0.10.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.3.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.18.112",
    "@types/pdfmake": "^0.2.7",
    "@types/sharp": "^0.32.0",
    "@types/useragent": "^2.3.3",
    "@types/uuid": "^10.0.0",
    "chai": "^5.1.1",
    "chai-http": "^5.0.0",
    "eslint": "^8.57.1",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.2",
    "uglifyjs-folder": "^3.3.0"
  }
}
